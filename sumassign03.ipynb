   "source": [
    "print('Starting of the program for sum of all numbers'.center(80,'*'))\n",
    "def sum(*args):\n",
    "    sum1= 0\n",
    "    for i in args:\n",
    "        sum1 = i + sum1\n",
    "    return sum1\n",
    "print(sum(8,2,3,0,7))\n",
    "print('THE END'.center(80,'*'))"
